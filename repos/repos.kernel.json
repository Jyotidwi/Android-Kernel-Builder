[
  {
    "kernelSource": {
      "name": "M215F_KernelSU",
      "repo": "https://github.com/Jyotidwi/M215F-S",
      "branch": "build",
      "device": "exynos9610-m21dd",
      "defconfig": "exynos9610-m21dd_defconfig"
    },
    "withKernelSU": true,
    "toolchains": [
      {
        "repo": "https://github.com/vijaymalav564/vortex-clang",
        "branch": "master",
        "name": "vortex-clang",
        "binPath": ["bin"]
      }
    ],
    "ccache": true,
    "params": {
      "ARCH": "arm64",
      "CC": "vortex-clang/bin/clang",
      "externalCommand": {
        "CROSS_COMPILE": "vortex-clang/bin/aarch64-linux-gnu-",
        "CROSS_COMPILE_ARM32": "vortex-clang/bin/arm-linux-gnueabi-"
      }
    },
    "AnyKernel3": {
      "use": true,
      "release": true,
      "repo": "https://github.com/easterNday/AnyKernel3/",
      "branch": "thyme"
    }
  }
]
