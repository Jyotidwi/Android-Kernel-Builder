[
  {
    "kernelSource": {
      "name": "M215F_KernelSU",
      "repo": "https://github.com/Jyotidwi/M215F-S",
      "branch": "build",
      "device": "exynos9610-m21dd",
      "defconfig": "exynos9610-m21dd_defconfig"
    },
    "withKernelSU": true,
    "toolchains": [
      {
        "repo": "https://github.com/kdrag0n/proton-clang",
        "branch": "master",
        "name": "proton",
        "binPath": ["bin"]
      }
    ],
    "ccache": true,
    "params": {
      "ARCH": "arm64",
      "CC": "proton/bin/clang",
      "externalCommand": {
        "CROSS_COMPILE": "proton/bin/aarch64-linux-android-",
        "CROSS_COMPILE_ARM32": "proton/bin/arm-linux-gnueabi-",
        "AR": "proton/bin/llvm-ar",
        "NM": "proton/bin/llvm-nm",
        "OBJCOPY": "proton/bin/llvm-objcopy",
        "OBJDUMP": "proton/bin/llvm-objdump",
        "STRIP": "proton/bin/llvm-strip"
      }
    },
    "AnyKernel3": {
      "use": true,
      "release": true,
      "repo": "https://github.com/easterNday/AnyKernel3/",
      "branch": "thyme"
    }
  }
]
